{
  "_comment": "开发环境基准测试配置 - 快速验证用",
  "database": {
    "name": "pgbench_test",
    "host": "localhost",
    "port": 5432,
    "user": "postgres"
  },
  "test_data": {
    "scale_factor": 10,
    "description": "约 100 万行数据，适合快速验证"
  },
  "benchmark": {
    "clients": [8, 16],
    "threads": 8,
    "duration": 60,
    "warmup_duration": 30
  },
  "tests": [
    {
      "name": "baseline",
      "description": "标准 TPC-B 测试",
      "enabled": true,
      "options": []
    }
  ],
  "monitoring": {
    "enabled": false,
    "interval": 5,
    "output_prefix": "dev"
  },
  "output": {
    "directory": "./results/dev",
    "include_timestamp": true,
    "analyze_latency": false,
    "extract_metrics": true
  }
}
